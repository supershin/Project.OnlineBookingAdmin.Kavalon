<div class="row">
    <label class="col-lg-2 text-right control-label">Project</label>
    <div class="col-lg-3">
        <select id="ProjectID" class="" name="ProjectID">
            @foreach (var item in (List
            <SelectListItem>
                )ViewBag.ProjectPermissionSelectList)
                {
                <option value="@item.Value">@item.Text</option>
                }
        </select>
    </div>
</div>
<div class="row">
    <label class="col-lg-2 text-right control-label">Booking Date</label>
    <div class="col-lg-3">
        <input id="StartDate" name="StartDate" type="text" class="form-control" value="" />
    </div>
    <label class="col-lg-1 text-center control-label" style="width:40px;">To</label>
    <div class="col-lg-3">
        <input id="EndDate" name="EndDate" type="text" class="form-control" value="" />
    </div>
</div>