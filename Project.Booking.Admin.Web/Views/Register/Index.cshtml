@model IEnumerable<RegisterViewModel>
@{
    ViewData["Title"] = "Register";
    ViewBag.Active = "Register";
    Layout = "~/Views/Shared/_LayoutSitePage.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h3 class="page-header">@ViewData["Title"]</h3>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-warning">
            <div class="panel-heading " style="display:flex;">
                <div class="card-title">
                    <h5>Register List</h5>
                </div>
                <div class="col-auto ml-auto d-print-none">
                    <a asp-action="Manage" class="btn btn-primary">Create Register</a>
                </div>
            </div>            
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive" id="partial-list">
                    <partial name="Partial_List"  />
                </div>
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>

@section Scripts{
    <script src="~/js/pages/register.js" asp-append-version="true" type="text/javascript"></script>
    <script>        
        $(document).ready(function () {
           register.init();
        });
    </script>
}


