@model DashboardViewModel
<form id="form-dashboard-search" autocomplete="off">
    <div class="col-lg-3 col-md-12">
        <select id="BUIDs" class="" data-placeholder="-- Select BU Group --">
            @foreach (var item in Model.BUList)
            {
                <option value="@item.ID">@item.Name</option>
            }
        </select>
    </div>
    <div class="col-lg-3 col-md-12">
        <select id="ProjectIDs" class="" data-placeholder="-- Select Project All --">
            <option value="">-- Select Project All --</option>
            @foreach (var item in Model.ProjectList)
            {
                <option value="@item.ProjectID">@item.ProjectName</option>
            }
        </select>
    </div>
    <div class="col-lg-3 col-md-6 col-xs-6">
        <input id="StartDate" name="StartDate" type="text" class="form-control" value="" placeholder="วันที่ Order (from)" />
    </div>
    <div class="col-lg-3 col-md-6 col-xs-6">
        <div class="input-group">
            <input id="EndDate" name="EndDate" type="text" class="form-control" placeholder="วันที่ Order (to)" />
            <span class="input-group-btn">
                <button typeof="submit" class="btn btn-danger">
                    <i class="fa fa-refresh"></i>
                </button>
            </span>
        </div>
    </div>
</form>